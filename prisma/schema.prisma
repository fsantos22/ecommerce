generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(cuid())
  firstName String
  lastName  String
  email     String   @unique
  password  String
  active    Boolean? @default(false)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}